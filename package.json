{
  "name": "templete_api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node ./dist/index.js",
    "cron": "node ./dist/cron.js",
    "queue": "node ./dist/queue.js",
    "transpile": "babel src --out-dir dist --copy-files",
    "clean": "rimraf dist",
    "init": "npm run migrate && npm run seed",
    "migrate": "sequelize-cli db:migrate",
    "rollback": "sequelize-cli db:migrate:undo:all",
    "undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all --debug",
    "seed-rollback": "sequelize-cli db:seed:undo:all",
    "build": "npm run clean && npm run transpile",
    "dev": "nodemon --watch src --exec ./src/index.js",
    "cron-dev": "nodemon --watch src --exec \"eslint src --fix && babel-node\" ./src/cron.js",
    "queue-dev": "nodemon --watch src --exec \"eslint src --fix && babel-node\" ./src/queue.js",
    "migrate-script-dev": "nodemon --watch src --exec \"eslint src --fix && babel-node\" ./src/scripts/create-table-script.js",
    "seed-script-dev": "nodemon --watch src --exec \"eslint src --fix && babel-node\" ./src/scripts/seed-data-script.js seedData",
    "lintfix": "eslint src --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.9",
    "dotenv": "^16.4.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "html-to-json-parser": "^2.0.0",
    "jsonexport": "^3.2.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lumie": "^0.1.2",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-http-bearer": "^1.0.1",
    "redis": "^3.0.2",
    "sequelize": "^6.33.0",
    "sequelize-noupdate-attributes": "^1.0.0"
  }
}
